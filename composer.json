{
    "name": "wyrihaximus/ratchet",
    "description": "Ratchet websockets integration plugin for CakePHP",
    "homepage": "http://wyrihaximus.net/projects/cakephp/ratchet.html",
    "license": "MIT",
    "keywords": [
        "cakephp",
        "ratchet",
        "websockets",
        "cakephp3"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "cakephp/cakephp": "^3.0",
        "cboden/Ratchet": "^0.3",
        "react/promise": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Ratchet\\": "src/"
        }
    },
    "suggest": {
        "mkraemer/react-pcntl": "Add support for OS signaling"
    },
    "require-dev": {
        "cakephp/debug_kit": "3.0.*@dev",
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^1.5.0"
    }
}
